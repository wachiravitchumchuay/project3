<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://project3.demo/schema" xmlns="http://project3.demo/schema">

	<xs:element name="getRestaurantRequest">
		<xs:complexType>
			<xs:sequence/>
		</xs:complexType>
	</xs:element>
	<xs:element name="getRestaurantResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="restaurants" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="carbohydrates" type="xs:string"/>
							<xs:element name="restaurant_nationality" type="xs:string"/>
							<xs:element name="district" type="xs:string"/>
							<xs:element name="protein" type="xs:string"/>
							<xs:element name="fat" type="xs:string"/>
							<xs:element name="restaurant_type" type="xs:string"/>
							<xs:element name="food_type" type="xs:string"/>
							<xs:element name="clean_max_budget" type="xs:string"/>
							<xs:element name="clean_min_budget" type="xs:string"/>
							<xs:element name="restaurant_name" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="getRestaurantRecommendationRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="runnerType" type="xs:string"/>
				<xs:element name="restaurantType" type="xs:string"/>
				<xs:element name="protein" type="xs:string"/>
				<xs:element name="fat" type="xs:string"/>
				<xs:element name="carb" type="xs:string"/>
				<xs:element name="budgetMin" type="xs:string"/>
				<xs:element name="budgetMax" type="xs:string"/>
				<xs:element name="foodTypes" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="foodType" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="getRestaurantRecommendationResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="restaurants" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="carbohydrates" type="xs:string"/>
							<xs:element name="restaurant_nationality" type="xs:string"/>
							<xs:element name="district" type="xs:string"/>
							<xs:element name="protein" type="xs:string"/>
							<xs:element name="fat" type="xs:string"/>
							<xs:element name="restaurant_type" type="xs:string"/>
							<xs:element name="food_type" type="xs:string"/>
							<xs:element name="clean_max_budget" type="xs:string"/>
							<xs:element name="clean_min_budget" type="xs:string"/>
							<xs:element name="restaurant_name" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>
